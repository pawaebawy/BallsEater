{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@data/*": [
        "data/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@modals/*": [
        "modals/*"
      ],
    }
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/**/*.js",
    "./src/**/*.jsx",
    "./App.tsx"
  ]
}
